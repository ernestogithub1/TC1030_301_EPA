
#include iostream

class Superheroe
{
private:
string alias, nombre

public:
string darNombreAlias();
void setAlias(string _alias);
void setNombre(string _nombre);
string str();

string Superheroe :: darNombreAlias( ){
return alias;
}
void Superheroe :: setAlias(string _alias){
alias = _alias;
}
void Superheroe :: setNombre(string _nombre){
nombre = _nombre;
}
string Superheroe :: str( ){
 return "alias="+ to_string(alias);
}

};
